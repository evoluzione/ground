{
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "warn",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        'max-len': [
            "error",
            {
                "code": 120,
                "ignoreUrls": true,
                "ignoreStrings": true,
                "ignoreComments": true,
                "ignoreTemplateLiterals": true
            }
        ],
        "padded-blocks": [
            "error",
            "always"
        ],
        space-before-function-paren: [
            "error",
            "never"
        ],
        space-in-parens: [
            "error",
            "never"
        ],
        no-unused-vars: "off",
        no-console: "warn"
    },
    "globals": {
        "TimelineLite": false,
        "TimelineMax": false,
        "TweenLite": false,
        "TweenMax": false,
        "Back": false,
        "Bounce": false,
        "Circ": false,
        "Cubic": false,
        "Ease": false,
        "EaseLookup": false,
        "Elastic": false,
        "Expo": false,
        "Linear": false,
        "Power0": false,
        "Power1": false,
        "Power2": false,
        "Power3": false,
        "Power4": false,
        "Quad": false,
        "Quart": false,
        "Quint": false,
        "RoughEase": false,
        "Sine": false,
        "SlowMo": false,
        "SteppedEase": false,
        "Strong": false,
        "Draggable": false,
        "SplitText": false,
        "VelocityTracker": false,
        "CSSPlugin": false,
        "ThrowPropsPlugin": false,
        "BezierPlugin": false,
        "ScrollMagic": false,
        "Barba": false,
        "Promise": false,
        "Yacc": false
    }
}
