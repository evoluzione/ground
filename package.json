{
	"name": "ground",
	"version": "2.0.0",
	"description": "WordPress starter theme for front-end developers",
	"license": "MIT",
	"scripts": {
		"start": "npm-run-all -p \"styles -- --watch\" \"js -- --mode=development\" sync",
		"build": "npm-run-all \"styles -- --env production\" \"js -- --mode=production\"",
		"styles": "postcss css/main.css -o css/styles.min.css --verbose",
		"sync": "browser-sync start -p 'https://ground.develop' -f 'css/styles.min.css' '**/*.php' 'js/scripts.min.js' -w",
		"js": "webpack"
	},
	"homepage": "https://github.com/fabioquarantini/ground",
	"author": {
		"name": "Fabio Quarantini",
		"url": "http://www.fabioquarantini.com"
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@babel/core": "^7.13.14",
		"@babel/eslint-parser": "^7.13.14",
		"@babel/preset-env": "^7.13.12",
		"@babel/preset-react": "^7.13.13",
		"@tailwindcss/aspect-ratio": "^0.2.0",
		"@tailwindcss/line-clamp": "^0.2.0",
		"@tailwindcss/forms": "^0.3.2",
		"@tailwindcss/typography": "^0.4.0",
		"autoprefixer": "^10.2.5",
		"babel-loader": "^8.2.2",
		"browser-sync": "^2.26.14",
		"core-js": "^3.9.1",
		"cross-env": "^7.0.3",
		"cssnano": "^4.1.10",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.23.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.8",
		"postcss-100vh-fix": "^1.0.2",
		"postcss-cli": "^8.3.1",
		"postcss-for": "^2.1.1",
		"postcss-import": "^14.0.0",
		"postcss-nested": "^4.2.3",
		"postcss-scss": "^3.0.5",
		"postcss-simple-vars": "^6.0.3",
		"prettier": "2.2.1",
		"tailwindcss": "^2.0.4",
		"tailwindcss-debug-screens": "^2.0.0",
		"webpack": "^5.28.0",
		"webpack-build-notifier": "^2.1.1",
		"webpack-cli": "^4.6.0"
	},
	"dependencies": {
		"@dogstudio/highway": "2.2.2",
		"deepmerge": "4.2.2",
		"gsap": "npm:@gsap/shockingly@^3.6.1",
		"imagesloaded": "4.1.4",
		"infinite-scroll": "3.0.6",
		"ismobilejs": "1.1.1",
		"locomotive-scroll": "4.1.1",
		"photoswipe": "4.1.3",
		"swiper": "6.5.2"
	}
}
