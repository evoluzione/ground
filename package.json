{
	"name": "ground",
	"version": "2.0.40",
	"description": "WordPress starter theme for front-end developers",
	"license": "MIT",
	"scripts": {
		"start": "npm run clean && npm-run-all -p \"styles -- --watch\" \"js -- --mode=development\" sync",
		"build": "npm-run-all clean \"js -- --mode=production\" \"styles -- --env production\"",
		"styles": "postcss assets/css/main-wc.css -o dist/css/ground-styles.min.css --verbose --map",
		"sync": "browser-sync start -p 'https://ground.develop' -f 'dist/css/ground-styles.min.css' '**/*.php' 'dist/js/ground-scripts.min.js' -w",
		"js": "webpack",
		"clean": "rm -rf ./dist/"
	},
	"homepage": "https://github.com/fabioquarantini/ground",
	"author": {
		"name": "Fabio Quarantini",
		"url": "http://www.fabioquarantini.com"
	},
	"browserslist": ["defaults"],
	"devDependencies": {
		"@babel/core": "^7.21.8",
		"@babel/eslint-parser": "^7.21.8",
		"@babel/preset-env": "^7.21.5",
		"@babel/preset-react": "^7.18.6",
		"@evoluzione/tailwind-config": "^1.0.16",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.2",
		"browser-sync": "^2.29.1",
		"classnames": "2.3.2",
		"core-js": "^3.30.1",
		"cross-env": "^7.0.3",
		"cssnano": "^6.0.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.23",
		"postcss-100vh-fix": "^1.0.2",
		"postcss-cli": "^10.1.0",
		"postcss-for": "^2.1.1",
		"postcss-import": "^15.1.0",
		"postcss-scss": "^4.0.6",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "2.8.8",
		"prettier-plugin-tailwindcss": "0.2.8",
		"tailwindcss": "^3.3.2",
		"webpack": "^5.82.0",
		"webpack-build-notifier": "^2.3.0",
		"webpack-cli": "^5.0.2"
	},
	"dependencies": {
		"deepmerge": "4.3.1",
		"gsap": "npm:@gsap/shockingly@^3.11.5",
		"infinite-scroll": "4.0.1",
		"ismobilejs": "1.1.1",
		"photoswipe": "5.3.7",
		"swiper": "9.2.4"
	}
}
