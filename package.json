{
	"name": "ground",
	"version": "1.1.0",
	"description": "WordPress starter theme for front-end developers",
	"license": "MIT",
	"scripts": {
		"start": "concurrently 'node-sass -w -o --source-map true --error-bell scss/main.scss css/main.css' 'npm:sync' 'webpack --mode=development'",
		"build": "concurrently 'node-sass -w -o --source-map true --error-bell --output-style compressed scss/main.scss css/main.css' 'webpack --mode=production'",
		"sync": "browser-sync start -p 'https://ground.develop' -f 'css/main.css' '**/*.php' 'js/scripts.min.js' -w"
	},
	"homepage": "https://github.com/fabioquarantini/ground",
	"author": {
		"name": "Fabio Quarantini",
		"url": "http://www.fabioquarantini.com"
	},
	"browserslist": [
		"last 2 versions",
		"> 25%",
		"ie >= 11"
	],
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"autoprefixer": "^10.2.1",
		"babel-loader": "^8.2.2",
		"browser-sync": "^2.26.13",
		"concurrently": "^5.3.0",
		"core-js": "^3.8.2",
		"cssnano": "^4.1.10",
		"node-sass": "^4.14.1",
		"postcss": "^8.2.4",
		"postcss-cli": "^8.3.1",
		"postcss-simple-vars": "^6.0.2",
		"webpack": "^5.14.0",
		"webpack-build-notifier": "^2.1.1",
		"webpack-cli": "^4.3.1"
	},
	"dependencies": {
		"@dogstudio/highway": "2.2.1",
		"coffeekraken-gridle": "3.0.3",
		"deepmerge": "4.2.2",
		"gsap": "npm:@gsap/shockingly@^3.6.0",
		"imagesloaded": "4.1.4",
		"include-media": "1.4.9",
		"infinite-scroll": "3.0.6",
		"ismobilejs": "1.1.1",
		"lazysizes": "5.3.0",
		"locomotive-scroll": "4.0.6",
		"normalize.css": "8.0.1",
		"photoswipe": "4.1.3",
		"rellax": "1.12.1",
		"ritmo": "0.5.0",
		"splitting": "1.0.6",
		"swiper": "6.3.2"
	}
}
