{
	"name": "ground",
	"version": "2.0.0",
	"description": "WordPress starter theme for front-end developers",
	"license": "MIT",
	"scripts": {
		"start": "npm-run-all -p \"styles -- --watch\" \"js -- --mode=development\" sync",
		"build": "npm-run-all \"styles -- --env production\" \"js -- --mode=production\"",
		"styles": "postcss css/main.css -o css/styles.min.css --verbose",
		"sync": "browser-sync start -p 'http://ground.develop' -f 'css/styles.min.css' '**/*.php' 'js/scripts.min.js' -w",
		"js": "webpack"
	},
	"homepage": "https://github.com/fabioquarantini/ground",
	"author": {
		"name": "Fabio Quarantini",
		"url": "http://www.fabioquarantini.com"
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"autoprefixer": "^10.2.3",
		"babel-loader": "^8.2.2",
		"browser-sync": "^2.26.13",
		"core-js": "^3.8.3",
		"cssnano": "^4.1.10",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.4",
		"postcss-100vh-fix": "^1.0.2",
		"postcss-cli": "^8.3.1",
		"postcss-for": "^2.1.1",
		"postcss-import": "^14.0.0",
		"postcss-nested": "^4.2.3",
		"postcss-scss": "^3.0.4",
		"postcss-simple-vars": "^6.0.2",
		"webpack": "^5.16.0",
		"webpack-build-notifier": "^2.1.1",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"@dogstudio/highway": "2.2.1",
		"@tailwindcss/aspect-ratio": "^0.2.0",
		"@tailwindcss/forms": "^0.2.1",
		"@tailwindcss/typography": "^0.4.0",
		"deepmerge": "4.2.2",
		"gsap": "npm:@gsap/shockingly@^3.6.0",
		"imagesloaded": "4.1.4",
		"infinite-scroll": "3.0.6",
		"ismobilejs": "1.1.1",
		"locomotive-scroll": "4.0.6",
		"photoswipe": "4.1.3",
		"swiper": "6.4.7",
		"tailwindcss": "^2.0.2"
	}
}
