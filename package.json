{
	"name": "ground",
	"version": "2.0.20",
	"description": "WordPress starter theme for front-end developers",
	"license": "MIT",
	"scripts": {
		"start": "npm run clean && npm-run-all -p \"styles -- --watch\" \"js -- --mode=development\" sync",
		"build": "npm-run-all clean \"js -- --mode=production\" \"styles -- --env production\"",
		"styles": "postcss assets/css/main-wc.css -o dist/css/ground-styles.min.css --verbose --map",
		"sync": "browser-sync start -p 'https://ground.develop' -f 'dist/css/ground-styles.min.css' '**/*.php' 'dist/js/ground-scripts.min.js' -w",
		"js": "webpack",
		"clean": "rm -rf ./dist/"
	},
	"homepage": "https://github.com/fabioquarantini/ground",
	"author": {
		"name": "Fabio Quarantini",
		"url": "http://www.fabioquarantini.com"
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@evoluzione/tailwind-config": "^1.0.14",
		"autoprefixer": "^10.4.13",
		"babel-loader": "^9.1.0",
		"browser-sync": "^2.27.10",
		"classnames": "2.3.2",
		"core-js": "^3.26.1",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-react-hooks": "^4.6.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.19",
		"postcss-100vh-fix": "^1.0.2",
		"postcss-cli": "^10.1.0",
		"postcss-for": "^2.1.1",
		"postcss-import": "^15.0.0",
		"postcss-scss": "^4.0.6",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "2.8.0",
		"prettier-plugin-tailwindcss": "0.2.0",
		"tailwindcss": "^3.2.4",
		"webpack": "^5.75.0",
		"webpack-build-notifier": "^2.3.0",
		"webpack-cli": "^5.0.0"
	},
	"dependencies": {
		"deepmerge": "4.2.2",
		"gsap": "npm:@gsap/shockingly@^3.11.3",
		"infinite-scroll": "4.0.1",
		"ismobilejs": "1.1.1",
		"photoswipe": "5.3.4",
		"swiper": "8.4.5"
	}
}
