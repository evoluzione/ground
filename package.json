{
	"name": "ground",
	"version": "2.0.0",
	"description": "WordPress starter theme for front-end developers",
	"license": "MIT",
	"scripts": {
		"start": "npm-run-all -p \"styles -- --watch\" \"js -- --mode=development\" sync",
		"build": "npm-run-all \"styles -- --env production\" \"js -- --mode=production\"",
		"styles": "postcss css/main.css -o css/styles.min.css --verbose --map",
		"sync": "browser-sync start -p 'https://ground.develop' -f 'css/styles.min.css' '**/*.php' 'js/scripts.min.js' -w",
		"js": "webpack"
	},
	"homepage": "https://github.com/fabioquarantini/ground",
	"author": {
		"name": "Fabio Quarantini",
		"url": "http://www.fabioquarantini.com"
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/eslint-parser": "^7.16.5",
		"@babel/preset-env": "^7.16.5",
		"@babel/preset-react": "^7.16.5",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/line-clamp": "^0.3.0",
		"@tailwindcss/typography": "^0.5.0",
		"autoprefixer": "^10.4.0",
		"babel-loader": "^8.2.3",
		"browser-sync": "^2.27.7",
		"core-js": "^3.20.1",
		"cross-env": "^7.0.3",
		"cssnano": "^5.0.14",
		"eslint": "^8.5.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.5",
		"postcss-100vh-fix": "^1.0.2",
		"postcss-cli": "^9.1.0",
		"postcss-for": "^2.1.1",
		"postcss-import": "^14.0.2",
		"postcss-scss": "^4.0.2",
		"postcss-simple-vars": "^6.0.3",
		"prettier": "2.5.1",
		"tailwindcss": "^3.0.8",
		"tailwindcss-debug-screens": "^2.2.1",
		"webpack": "^5.65.0",
		"webpack-build-notifier": "^2.3.0",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"@dogstudio/highway": "2.2.1",
		"dat.gui": "^0.7.7",
		"deepmerge": "4.2.2",
		"gsap": "npm:@gsap/shockingly@^3.9.1",
		"imagesloaded": "4.1.4",
		"infinite-scroll": "4.0.1",
		"ismobilejs": "1.1.1",
		"photoswipe": "4.1.3",
		"swiper": "7.4.1",
		"three": "^0.136.0"
	}
}
