{
	"name": "ground",
	"version": "2.0.0",
	"description": "WordPress starter theme for front-end developers",
	"license": "MIT",
	"scripts": {
		"start": "concurrently 'postcss css/main.css -o css/styles.min.css --watch --verbose --map' 'npm:sync' 'webpack --mode=development'",
		"build": "concurrently 'postcss css/main.css -o css/styles.min.css --verbose --map --env \"production\"' 'webpack --mode=production'",
		"sync": "browser-sync start -p 'https://ground.develop' -f 'css/styles.min.css' '**/*.php' 'js/scripts.min.js' 'img/**/*.*' --w"
	},
	"homepage": "https://github.com/fabioquarantini/ground",
	"author": {
		"name": "Fabio Quarantini",
		"url": "http://www.fabioquarantini.com"
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.12.1",
		"autoprefixer": "^10.2.1",
		"babel-loader": "^8.1.0",
		"browser-sync": "^2.26.13",
		"concurrently": "^5.3.0",
		"core-js": "^3.6.5",
		"cssnano": "^4.1.10",
		"postcss": "^8.2.4",
		"postcss-100vh-fix": "^1.0.2",
		"postcss-cli": "^8.3.1",
		"postcss-for": "^2.1.1",
		"postcss-import": "^14.0.0",
		"postcss-nested": "^5.0.3",
		"postcss-scss": "^3.0.4",
		"postcss-simple-vars": "^6.0.2",
		"webpack": "^4.44.2",
		"webpack-build-notifier": "^2.1.0",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"@dogstudio/highway": "2.2.1",
		"@tailwindcss/aspect-ratio": "^0.2.0",
		"@tailwindcss/forms": "^0.2.1",
		"@tailwindcss/typography": "^0.3.1",
		"deepmerge": "4.2.2",
		"gsap": "file:gsap-bonus.tgz",
		"imagesloaded": "4.1.4",
		"infinite-scroll": "3.0.6",
		"ismobilejs": "1.1.1",
		"lazysizes": "5.3.0",
		"locomotive-scroll": "4.0.6",
		"photoswipe": "4.1.3",
		"rellax": "1.12.1",
		"splitting": "1.0.6",
		"swiper": "6.4.5",
		"tailwindcss": "^2.0.2"
	}
}
