{
	"name": "ground",
	"version": "2.0.0",
	"description": "WordPress starter theme for front-end developers",
	"license": "MIT",
	"scripts": {
		"start": "npm run clean && npm-run-all -p \"styles -- --watch\" \"js -- --mode=development\" sync",
		"build": "npm-run-all clean \"js -- --mode=production\" \"styles -- --env production\"",
		"styles": "postcss assets/css/main-wc.css -o dist/css/ground-styles.min.css --verbose --map",
		"sync": "browser-sync start -p 'https://ground.develop' -f 'dist/css/ground-styles.min.css' '**/*.php' 'dist/js/ground-scripts.min.js' -w",
		"js": "webpack",
		"clean": "rm -rf ./dist/"
	},
	"homepage": "https://github.com/fabioquarantini/ground",
	"author": {
		"name": "Fabio Quarantini",
		"url": "http://www.fabioquarantini.com"
	},
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@babel/core": "^7.18.2",
		"@babel/eslint-parser": "^7.18.2",
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-react": "^7.17.12",
		"@evoluzione/tailwind-config": "^1.0.12",
		"autoprefixer": "^10.4.7",
		"babel-loader": "^8.2.5",
		"browser-sync": "^2.27.10",
		"classnames": "2.3.1",
		"core-js": "^3.22.7",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.10",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.30.0",
		"eslint-plugin-react-hooks": "^4.5.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.14",
		"postcss-100vh-fix": "^1.0.2",
		"postcss-cli": "^9.1.0",
		"postcss-for": "^2.1.1",
		"postcss-import": "^14.1.0",
		"postcss-scss": "^4.0.4",
		"postcss-simple-vars": "^6.0.3",
		"prettier": "2.6.2",
		"prettier-plugin-tailwindcss": "0.1.11",
		"tailwindcss": "^3.0.24",
		"webpack": "^5.72.1",
		"webpack-build-notifier": "^2.3.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"deepmerge": "4.2.2",
		"gsap": "npm:@gsap/shockingly@^3.10.4",
		"infinite-scroll": "4.0.1",
		"ismobilejs": "1.1.1",
		"photoswipe": "4.1.3",
		"swiper": "8.2.1"
	}
}
